        <input type="text" name="location" id="">
        <form action="/user/"<%user.sex%>                ?_method="PUT"method="POST"></form>
        <input type="text" name="sex" id="">
        <form action="/user/"<%user.img%>                ?_method="PUT"method="POST"></form>
        <input type="text" name="img" id="">
    </main>
</body>
</html>
